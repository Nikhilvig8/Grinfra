<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title</title>

    <!-- Web Fonts -->
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet" />
    <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.9.0/css/all.css" rel="stylesheet" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <link href="~/font/flaticon.css" rel="stylesheet" />
    <link href="~/font/helvetica-neue/style.css" rel="stylesheet" />
    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css" />

    <!-- Latest compiled and minified CSS -->
    <link href="https://cdnjs.cloudflare.com/ajax/libs/select2/4.0.6-rc.0/css/select2.min.css" rel="stylesheet" />

    <!-- DatePicker CSS -->
    <link rel="stylesheet" type="text/css" href="https://www.jqueryscript.net/demo/Date-Time-Picker-Bootstrap-4/build/css/bootstrap-datetimepicker.min.css" />

    <!-- custom scrollbar -->
    <link rel="stylesheet" href="~/Content/jquery.mCustomScrollbar.min.css" />
    <!-- Custom CSS -->
    <link href="~/Content/style.css" rel="stylesheet" />
    <link href="~/Content/common.css" rel="stylesheet" />
    <link rel="stylesheet" type="text/css" href="https://www.jqueryscript.net/demo/Date-Time-Picker-Bootstrap-4/build/css/bootstrap-datetimepicker.min.css" />
    <style>
        .select2 {
            width: 100% !important;
        }

        .select2-container--default .select2-selection--single {
            height: auto !important;
            padding: 0.375rem 0.75rem !important;
            line-height: 1.5;
        }

            .select2-container--default .select2-selection--single .select2-selection__rendered {
                line-height: 1.5 !important;
                padding: 0 !important;
            }

            .select2-container--default .select2-selection--single .select2-selection__arrow b {
                display: none;
            }

        .nicescroll-cursors {
            background-color: #eee !important;
            visibility: hidden;
        }
    </style>
    @*<link href="~/Content/select2.min.css" rel="stylesheet" />
        <link href="~/Content/select2-bootstrap.css" rel="stylesheet" />
        <link href="~/Content/pmd-select2.css" rel="stylesheet" />*@
</head>
<body>
    <div class="wrapper">
        <!--SideBar-->
        @Html.Partial("_MenuPartial")

        <!--Upto Here-->
        <div class="content" id="content">
            <header>
                <nav class="navbar navbar-expand-lg navbar-dark bg-dark fixed-top top-navbar">
                    <button type="button" id="sidebarCollapse" class="btn">
                        <svg class="svg-inline--fa fa-align-left fa-w-14" aria-hidden="true" data-prefix="fas" data-icon="align-left" role="img" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 448 512" data-fa-i2svg=""><path fill="currentColor" d="M288 44v40c0 8.837-7.163 16-16 16H16c-8.837 0-16-7.163-16-16V44c0-8.837 7.163-16 16-16h256c8.837 0 16 7.163 16 16zM0 172v40c0 8.837 7.163 16 16 16h416c8.837 0 16-7.163 16-16v-40c0-8.837-7.163-16-16-16H16c-8.837 0-16 7.163-16 16zm16 312h416c8.837 0 16-7.163 16-16v-40c0-8.837-7.163-16-16-16H16c-8.837 0-16 7.163-16 16v40c0 8.837 7.163 16 16 16zm256-200H16c-8.837 0-16 7.163-16 16v40c0 8.837 7.163 16 16 16h256c8.837 0 16-7.163 16-16v-40c0-8.837-7.163-16-16-16z"></path></svg><!-- <i class="fas fa-align-left"></i> -->
                    </button>
                    <p class="module-name"><span><img src="~/images/2020-02-04.png" height="40px" /></span></p>

                    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                        <span class="navbar-toggler-icon"></span>
                    </button>
                    <div class="collapse navbar-collapse top-nav-content" id="navbarSupportedContent">
                        <ul class="nav ml-auto">
                            <li class="nav-item dropdown">
                                <a class="nav-link dropdown-toggle" href="#" id="navbarDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                    <span class="bold-fonts">Welcome</span> <span class="bold-fonts">@Session["LoginId"]</span> <br />

                                    <i class="material-icons">
                                        keyboard_arrow_down
                                    </i>
                                </a>
                                <div class="dropdown-menu" aria-labelledby="navbarDropdown">
                                    <a class="dropdown-item" href="/LoginPortal/Logout">Logout</a>
                                </div>
                            </li>
                        </ul>
                    </div>
                </nav>
            </header>
            <div id="page-content-wrapper">
                <div class="container-fluid">
                    <div class="row">
                        <div class="col-md-12 content-width">
                            @RenderBody()
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>



    <footer>
        <p>© GRINFRA © 2019 All rights reserved. </p>
    </footer>
    <div class="overlay"></div>
    <div class="overlay phone"></div>
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)

    <!-- Optional JavaScript -->
    <!-- jQuery first, then Popper.js, then Bootstrap JS -->
    <!-- jQuery library -->

    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-cookie/1.4.1/jquery.cookie.js" type="text/javascript"></script>
    <!-- Popper JS -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.7/umd/popper.min.js"></script>
    <!-- Latest compiled JavaScript -->
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js"></script>

    <script src="https://cdnjs.cloudflare.com/ajax/libs/select2/4.0.6-rc.0/js/select2.min.js"></script>

    <script src="~/Scripts/jquery.mCustomScrollbar.concat.min.js"></script>
    <script src="~/Scripts/custom.js"></script>

    <script src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.21.0/moment.min.js" type="text/javascript"></script>
    <script type="text/javascript" src="https://www.jqueryscript.net/demo/Date-Time-Picker-Bootstrap-4/build/js/bootstrap-datetimepicker.min.js"></script>
    <script type="text/javascript" src="https://www.gstatic.com/charts/loader.js"></script>

    <script src="~/JS/freeze.js"></script>
    <script src="~/JS/NewEventRequest.js"></script>
    @*<script src="https://cdn.jsdelivr.net/jquery.validation/1.16.0/jquery.validate.min.js"></script>*@

    <script>
        $(document).ready(function () {
            $('.overlay').hide();
            $('#accordion .card-header, #accordion1 .card-header').click(function () {
                $(this).parent().parent().find('.card .card-header').addClass('back-grey');
                if ($(this).find('.btn').hasClass('collapsed')) {
                    $(this).removeClass('back-grey');
                }
                else {
                    $(this).addClass('back-grey');
                }
            });
            $('.sidebar-toggler').click(function () {
                $('.overlay').show();
                $('#sidebar-wrapper').addClass('open');
            });
            $('.overlay').click(function () {
                $('#sidebar-wrapper').removeClass('open');
                $(this).hide();
            });
            $('#sidebarCollapse').on('click', function () {
                $('#sidebar, #content, .top-navbar, .sidebar-nav  span').toggleClass('active');
                if ($(window).width() < 768) {
                    $('.sub-menu').removeClass('icon-view')
                } else {
                    $('.sub-menu').addClass('icon-view')
                }
                if ($('.sidebar-nav span').hasClass('active')) {
                    $('.sidebar-nav span.active').fadeOut();
                } else {
                    $('.sidebar-nav span').fadeIn();
                }
                $('.collapse.in').toggleClass('in');
                $('.overlay.phone').toggleClass('active');
            });
            $('.sidebar-nav').mCustomScrollbar();
        });
        $('.select2').select2();

    </script>
</body>
</html>